<ResourceDictionary
   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   xmlns:tl="clr-namespace:VentWPF.Tools">
   <!--  Конверторы  -->
   <tl:BrushConverter
      x:Key="brBR"
      FalseBrush="Red"
      TrueBrush="Black" />
   <tl:VisibilityConverter x:Key="visConv" />
   <tl:VisibilityConverter x:Key="visConvrev" Reverse="True" />
   <tl:BrushConverter
      x:Key="brNR"
      FalseBrush="{StaticResource PrimaryHueDarkBrush}"
      TrueBrush="Red" />
   <tl:BrushConverter
      x:Key="brGR"
      FalseBrush="LimeGreen"
      TrueBrush="Red" />
   <tl:MulConverter x:Key="frame-scale" K="0.3" />
   <tl:BrushConverter
      x:Key="brRG"
      TrueBrush="LimeGreen"
      FalseBrush="Red" />
   <tl:MarginConverter
      x:Key="toleft"
      Margin="5,0,0,0"
      Direction="0" />
   <tl:MarginConverter x:Key="toTop" Direction="1" />

</ResourceDictionary>